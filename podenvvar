apiVersion: v1
kind: Pod
metadata:
  creationTimestamp: null
  labels:
    run: nginx
  name: nginx
spec:
  containers:
  - image: nginx
    name: nginx
    resources: {}
    envFrom:
    - configMapRef:
        name: keycm
    env:
    - name: KEY3
      valueFrom:
        configMapKeyRef:
          name: envfile
          key: var3
  dnsPolicy: ClusterFirst
  restartPolicy: Always
status: {}
